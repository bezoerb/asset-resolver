{
  "name": "asset-resolver",
  "version": "2.0.4",
  "description": "Find an asset in a set of locations",
  "license": "MIT",
  "repository": "bezoerb/asset-resolver",
  "author": {
    "name": "Ben ZÃ¶rb",
    "email": "ben@sommerlaune.com",
    "url": "sommerlaune.com"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">= 8.6"
  },
  "scripts": {
    "ava": "nyc ava",
    "xo": "xo",
    "test": "npm run xo && npm run ava"
  },
  "files": [
    "index.js",
    "cli.js",
    "lib"
  ],
  "keywords": [
    "asset",
    "resolve"
  ],
  "dependencies": {
    "bluebird": "^3.7.0",
    "debug": "^4.1.1",
    "filesize": "^5.0.1",
    "fs-extra": "^8.1.0",
    "globby": "^9.2.0",
    "got": "^9.6.0",
    "hash": "^0.2.1",
    "meow": "5.0.0",
    "mime": "^2.4.4",
    "object": "0.1.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "es6-promise": "^4.2.8",
    "finalhandler": "^1.1.2",
    "get-port": "^5.0.0",
    "nyc": "^14.1.1",
    "serve-static": "^1.14.1",
    "xo": "^0.25.3"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "space": 2,
    "prettier": true,
    "rules": {
      "promise/valid-params": "off",
      "promise/prefer-await-to-then": "off",
      "capitalized-comments": "off"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
