{
  "name": "asset-resolver",
  "version": "3.0.5",
  "description": "Find an asset in a set of locations",
  "license": "MIT",
  "repository": "bezoerb/asset-resolver",
  "author": {
    "name": "Ben ZÃ¶rb",
    "email": "ben@sommerlaune.com",
    "url": "https://www.sommerlaune.com"
  },
  "bin": {
    "asset-resolver": "cli.js"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "scripts": {
    "ava": "nyc ava",
    "xo": "xo",
    "test": "npm run xo && npm run ava"
  },
  "files": [
    "index.js",
    "cli.js",
    "lib"
  ],
  "keywords": [
    "asset",
    "resolve"
  ],
  "dependencies": {
    "debug": "^4.3.1",
    "globby": "^11.0.1",
    "got": "^11.8.1",
    "meow": "^7.1.1",
    "mime": "^2.4.7",
    "normalize-path": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "finalhandler": "^1.1.2",
    "get-port": "^5.1.1",
    "nyc": "^15.1.0",
    "serve-static": "^1.14.1",
    "xo": "^0.36.1"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "space": 2,
    "prettier": true,
    "rules": {
      "no-await-in-loop": "off",
      "capitalized-comments": "off",
      "promise/prefer-await-to-then": "off"
    }
  },
  "prettier": {
    "useTabs": false
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
